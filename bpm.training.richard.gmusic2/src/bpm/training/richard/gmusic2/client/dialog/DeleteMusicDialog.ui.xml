<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<m:MaterialPanel>
		<m:MaterialModalContent>
			<m:MaterialTitle title="Confirm Delete" description="Are you sure you want to delete this song?"/>
		</m:MaterialModalContent>
		
		<m:MaterialModalFooter align="right">
			<m:MaterialButton ui:field="btnYes" waves="green" type="flat" text="Yes"  />
			<m:MaterialButton ui:field="btnNo" waves="red" type="flat" text="No"  />
		</m:MaterialModalFooter>
	</m:MaterialPanel>
	
</ui:UiBinder> 