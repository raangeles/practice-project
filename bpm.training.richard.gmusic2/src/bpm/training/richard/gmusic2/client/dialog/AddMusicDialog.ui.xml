<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui">
	<ui:style>
	
	</ui:style>
	<m:MaterialPanel>
		<m:MaterialModalContent>
			<m:MaterialTitle ui:field="modalTitle" title="Add Music" description="Please enter your music info and upload the file directly."/>
			<m:MaterialTextBox ui:field="txtTitle" placeholder="Title" />
			<m:MaterialTextArea ui:field="txtDescription" placeholder="Description" />
			<m:MaterialTextBox ui:field="txtAuthor" placeholder="Author" />
			<m:MaterialTextBox ui:field="txtPicture" placeholder="Picture URL"/>
			
		</m:MaterialModalContent>
				
		<m:MaterialModalFooter align="right">
			<m:MaterialButton ui:field="btnAdd" waves="green" type="flat" text="Add" />
			<m:MaterialButton ui:field="btnCancel" waves="red" type="flat" text="Cancel" />
		</m:MaterialModalFooter>
	</m:MaterialPanel>
</ui:UiBinder> 